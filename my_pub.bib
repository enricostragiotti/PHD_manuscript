@inproceedings{stragiotti_enhanced_2022,
  address   = {Leeds, United Kingdom},
  title     = {Enhanced truss topology optimization ({TTO}) applied to a cellular wing box},
  url       = {https://hal.science/hal-04283287},
  abstract  = {Cellular architectured structures' most appealing characteristic is that they are able to create large structures assembling small repetitive components. Thanks to their modular nature, they bring interesting features among which reduced tooling, fast assembly, and short repair time. In the first part of the article, we formulate an optimizing method that minimizes the mass of a cellular architectured structure taking into account internal stresses, local buckling, and pattern repetition constraints. The proposed solving algorithm mitigates the appearance of local minima, a critical problem for discrete trusses. In the second part, the optimization method is applied to a real-size aeronautic application: the minimization of the mass of a cellular 3D wing box subject to lift and torsion loads. Compared to classic cell topologies, the proposed method found a cell 10-times lighter, at the cost of increased manufacturing difficulty.},
  urldate   = {2023-11-14},
  booktitle = {{ASMO}-{UK} 12 / {ASMO}-{Europe} 1 / {ISSMO} {Conference} on {Engineering} {Design} {Optimization} (2022)},
  author    = {Stragiotti, Enrico and Irisarri, François-Xavier and Julien, Cédric and Morlier, Joseph},
  month     = jul,
  year      = {2022},
  keywords  = {Optimisation, Optimisation Topologique, Structure Lattice, conf},
  file      = {HAL PDF Full Text:D\:\\estragio\\Zotero\\storage\\ZY7S5YZA\\Stragiotti et al. - 2022 - Enhanced truss topology optimization (TTO) applied.pdf:application/pdf}
}

